#Prosedur Plotting Grafik Animasi
F:=proc(s) plots[display](
		plots[implicitplot](x^2+y^2=1,x=-1..1,y=-1..1,scaling=constrained),
		plottools[sector]([0,0],0.2,0..s+0.000001,color=red),
		plottools[point]([cos(s),sin(s)],color=blue,symbol=solidcircle,symbolsize=20),
		pointplot([0,0],view=[-1.15..1.15,-1.15..1.15]),
		plottools[line]([0,0],[cos(s),sin(s)],color=coral,thickness=3),
		plottools[line]([0,0],[cos(s),0],color=green,thickness=3),
		plottools[line]([cos(s),0],[cos(s),sin(s)],color=magenta,thickness=3),
		textplot([cos(s),sin(s),"A",'font'=["Arial Black",12]],'align'=above),
		textplot([cos(s)/2,sin(s)/2,"r",'font'=["Arial Black",12]]),
		textplot([cos(s)/2,0,"a",'font'=["Arial Black",12]]),
		textplot([cos(s),sin(s)/2,"b",'font'=["Arial Black",12]])
	); end proc:
