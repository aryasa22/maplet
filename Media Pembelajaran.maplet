				Set(MVSoal(value)=MathML[Export]("Jika Tan a&deg; = -1/2 dan a&deg; berada di kuadran di mana Cos dan Tan bernilai negatif, maka nilai dari 2 Sin a&deg; / 3 Cos a&deg; adalah..."));
				Set(TFNomor="17");
				Set(MA=MathML[Export](-1));
				Set(MB=MathML[Export](-2/(5^1/2)));
				Set(MC=MathML[Export](-1/3));
				Set(MD=MathML[Export](2/(5^(1/2))));
				Set(MF=MathML[Export](5^(1/2)));
			elif a=18 then
				Set(MVSoal(value)=MathML[Export]("Jika Cos 17&deg; = a, maka nilai dari Sin 73&deg; - Cos 163&deg; + Cos 343&deg; = ..."));
				Set(TFNomor="18");
				Set(MA=MathML[Export](1/a));
				Set(MB=MathML[Export](2*a));
				Set(MC=MathML[Export](3*a));
				Set(MD=MathML[Export](1-a));
				Set(MF=MathML[Export](a));
			elif a=19 then
				Set(MVSoal(value)=MathML[Export]("Nilai dari Sec (90&deg; - a) / Csc (180&deg;-a) adalah..."));
				Set(TFNomor="19");
				Set(MA=MathML[Export](-1));
				Set(MB=MathML[Export](1/2));
				Set(MC=MathML[Export](1));
				Set(MD=MathML[Export](0));
				Set(MF=MathML[Export](2^(1/2)));
			elif a=20 then
				Set(MVSoal(value)=MathML[Export]("Jika diketahui nilai Tan 26&deg; = t, maka berapakah nilai dari Tan 206&deg; - Tan 334&deg;?"));
				Set(TFNomor="20");
				Set(MA=MathML[Export](-t));
				Set(MB=MathML[Export](t/3));
				Set(MC=MathML[Export](3*t));
				Set(MD=MathML[Export](t));
				Set(MF=MathML[Export](0));
		end if;
		if a>=20 then Set(BNext(enabled)=false)
			else  Set(BNext(enabled)=true)
		end if;
		if a<=1 then Set(BPrev(enabled)=false) 
			else Set(BPrev(enabled)=true)
		end if;
	end proc:

