#Fungsi untuk tan pada tampilan materi
T1:=proc(a) if a=90 or a=270 then "Tak Terdefinisi" else evalf(tan(a*Pi/180),3) end if; end proc:
T2:=proc(a) if a=90 then "Tak Terdefinisi" else evalf(tan(a*Pi/180),3) end if; end proc:
T3:=proc(a) if a=270 then "Tak Terdefinisi" else evalf(tan(a*Pi/180),3) end if; end proc:

#Prosedur Mengacak Sudut
sdt:=proc() global a,correctsin,correctcos,correcttan;
		a:=op(sudut[sud()]);
		Set(TFSudut_Lat=a);
		Set(TFWarning_Lat=warning);
		Set(TFSin_Lat="");
		Set(TFCos_Lat="");
		Set(TFTan_Lat="");
		Set(B2_Lat(enabled)=true);
		Set(B3_Lat(enabled)=true);
		Set(TFSin_Lat(background)=pink);
		Set(TFCos_Lat(background)=pink);
		Set(TFTan_Lat(background)=pink);
		Set(TFWarning_Lat(background)=pink);
		correctsin:=sin(a*Pi/180);
		correctcos:=cos(a*Pi/180);
		if a = 90 or a = 270 then correcttan:="Tak Terdefinisi"
				else 
					correcttan:=tan(a*Pi/180) end if;
	end proc:

#Prosedur Cek Jawaban Latihan
cek:=proc() global a, correctsin,correctcos,correcttan;
	local usersin,usercos,usertan;
		usersin:=Get(TFSin_Lat::anything,corrections=true,update=true);
		usercos:=Get(TFCos_Lat::anything,corrections=true,update=true);
		usertan:=Get(TFTan_Lat::anything,corrections=true,update=true);
		if Get(TFSin_Lat)="" or Get(TFCos_Lat)="" or Get(TFTan_Lat)="" then
			Set(TFWarning_Lat="Silahkan jawab semuanya terlebih dahulu");return; end if;
		if usersin=correctsin and correctcos=usercos and usertan=correcttan then
			Set(TFWarning_Lat=cat(benar[ben()]),TFWarning_Lat(background)=green);
			Set(B3_Lat(enabled)=false)
				else
					Set(TFWarning_Lat=cat(salah[sal()]),TFWarning_Lat(background)=yellow);
					Set(B3_Lat(enabled)=true) end if;
		if usersin=correctsin then Set(TFSin_Lat(background)=green)
			else
				Set(TFSin_Lat(background)=yellow) end if;
		if usercos=correctcos then Set(TFCos_Lat(background)=green)
			else
				Set(TFCos_Lat(background)=yellow) end if;
		if usertan=correcttan then Set(TFTan_Lat(background)=green)
			else
				Set(TFTan_Lat(background)=yellow) end if;
	end proc:

#Prosedur Memunculkan Jawaban Latihan
show:=proc() global a, correctsin,correctcos,correcttan;
		correctsin:=sin(a*Pi/180);
		correctcos:=cos(a*Pi/180);
		if a = 90 then correcttan="Tak Terdefinisi"
			elif a = 270 then correcttan="Tak Terdefinisi"
				else 
					correcttan=tan(a*Pi/180) end if;
		Set(TFSin_Lat=correctsin,TFSin_Lat(background)="#FFCC99");
		Set(TFCos_Lat=correctcos,TFCos_Lat(background)="#FFCC99");
		Set(TFTan_Lat=correcttan,TFTan_Lat(background)="#FFCC99");
		Set(TFWarning_Lat=cat(nyerah[nyer()]),TFWarning_Lat(background)="#FFCC99");
		Set(B2_Lat(enabled)=false);
	end proc:

